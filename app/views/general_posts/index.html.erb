<h1><%= "General Forum" %></h1>
<%= link_to "Create A New Post", posts_new_path %>
<hr/>
<% @posts.each do |post|%>
    <p>
     <h4><%= post.title %></h4>
    </p>
    <p>
     <%= post.body %>
    </p>
    <div>
      <%= timestampParser(post.created_at) %>
    </div>
    <p>User's college: <%= User.find(post.user_id).university %></p>
    <%= link_to "Details", post %>
    <hr/>
    
 
<% end %> 